<p id="notice"><%= notice %></p>

<h1>Feedbacks</h1>

<table>
  <thead>
    <tr>
      <th>Project name</th>
      <th>App name</th>
      <th>App comp</th>
      <th>Feedback title</th>
      <th>Feedback desc</th>
      <th>Screenshot</th>
      <th>User</th>
      <th>Customer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.project_name %></td>
        <td><%= feedback.app_name %></td>
        <td><%= feedback.app_comp %></td>
        <td><%= feedback.feedback_title %></td>
        <td><%= feedback.feedback_desc %></td>
        <td><%= feedback.screenshot %></td>
        <td><%= feedback.user %></td>
        <td><%= feedback.customer %></td>
        <td><%= link_to 'Show', feedback %></td>
        <td><%= link_to 'Edit', edit_feedback_path(feedback) %></td>
        <td><%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feedback', new_feedback_path %>
