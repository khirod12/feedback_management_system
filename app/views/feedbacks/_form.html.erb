<%= form_for(feedback) do |f| %>
  <% if feedback.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(feedback.errors.count, "error") %> prohibited this feedback from being saved:</h2>

      <ul>
      <% feedback.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :project_name %>
    <%= f.text_field :project_name %>
  </div>

  <div class="field">
    <%= f.label :app_name %>
    <%= f.text_field :app_name %>
  </div>

  <div class="field">
    <%= f.label :app_comp %>
    <%= f.text_field :app_comp %>
  </div>

  <div class="field">
    <%= f.label :feedback_title %>
    <%= f.text_field :feedback_title %>
  </div>

  <div class="field">
    <%= f.label :feedback_desc %>
    <%= f.text_area :feedback_desc %>
  </div>

  <div class="field">
    <%= f.label :screenshot %>
    <%= f.text_field :screenshot %>
  </div>

  <div class="field">
    <%= f.label :user_id %>
    <%= f.text_field :user_id %>
  </div>

  <div class="field">
    <%= f.label :customer_id %>
    <%= f.text_field :customer_id %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
